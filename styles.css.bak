/* ========================================
   About Section
   ======================================== */

.about {
    padding: 100px 0;
    background: linear-gradient(180deg, var(--bg-dark) 0%, var(--secondary-color) 100%);
}

.about-content {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: center;
    margin-top: 60px;
}

.about-text p {
    font-size: 16px;
    line-height: 1.8;
    margin-bottom: 20px;
    color: var(--text-light);
}

.about-stats {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 30px;
    margin-top: 40px;
}

.stat {
    text-align: center;
    padding: 20px;
    background: rgba(var(--primary-rgb), 0.1);
    border-radius: 10px;
    border: 1px solid var(--border-color);
}

.stat h4 {
    font-size: 36px;
    color: var(--primary-color);
    margin-bottom: 10px;
}

.stat p {
    font-size: 14px;
    margin: 0;
}

.about-features {
    display: grid;
    gap: 25px;
}

.feature-item {
    display: flex;
    gap: 20px;
    align-items: flex-start;
}

.feature-item i {
    font-size: 24px;
    color: var(--primary-color);
    flex-shrink: 0;
    margin-top: 5px;
}

.feature-item h4 {
    font-size: 18px;
    margin-bottom: 8px;
    color: var(--text-light);
}

    position: relative;
    overflow: hidden;
}

.contact .container {
    position: relative;
    z-index: 1;
.feature-item p {
    font-size: 14px;
    color: var(--text-light);
    opacity: 0.8;
    margin: 0;
}

/* ========================================
   Contact Section
   ======================================== */

.contact {
    padding: 100px 0;
    background-color: var(--bg-dark);
}

.contact-content {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    margin-top: 60px;
    align-items: start;
}

.contact-info h3 {
    font-size: 28px;
    margin-bottom: 20px;
    color: var(--text-light);
}

.contact-info p {
    font-size: 16px;
    color: var(--text-light);
    margin-bottom: 40px;
    line-height: 1.8;
}

.contact-methods {
    display: grid;
    gap: 25px;
}

.contact-method {
    display: flex;
    gap: 20px;
    padding: 20px;
    background: rgba(var(--primary-rgb), 0.05);
    border: 1px solid var(--border-color);
    border-radius: 10px;
    cursor: pointer;
    transition: all 0.3s ease;
}

.contact-method:hover {
    border-color: var(--primary-color);
    background: rgba(var(--primary-rgb), 0.1);
    transform: translateX(10px);
}

.contact-method i {
    font-size: 24px;
    color: var(--primary-color);
    min-width: 24px;
}

.contact-method h4 {
    font-size: 16px;
    margin-bottom: 5px;
    color: var(--text-light);
}

.contact-method p {
    font-size: 14px;
    color: var(--text-light);
    opacity: 0.8;
    margin: 0;
}

/* ========================================
   Contact Form
   ======================================== */

.contact-form {
    display: grid;
    gap: 20px;
}

.form-group {
    display: flex;
}

.form-group input,
.form-group select,
.form-group textarea {
    width: 100%;
    padding: 15px;
    background: rgba(255, 255, 255, 0.05);
    border: 1px solid var(--border-color);
    border-radius: 5px;
    color: var(--text-light);
    font-size: 14px;
    font-family: inherit;
    transition: all 0.3s ease;
}

.form-group input::placeholder,
.form-group textarea::placeholder {
    color: rgba(224, 224, 224, 0.5);
}

.form-group input:focus,
.form-group select:focus,
.form-group textarea:focus {
    outline: none;
    border-color: var(--primary-color);
    background: rgba(var(--primary-rgb), 0.12);
    box-shadow: 0 0 10px rgba(var(--primary-rgb), 0.2);
}

.form-group select {
    cursor: pointer;
}

.form-group select option {
    background: var(--secondary-color);
    color: var(--text-light);
}

.submit-button {
    padding: 15px 50px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    color: white;
    border: none;
    border-radius: 5px;
    font-weight: 600;
    font-size: 16px;
    cursor: pointer;
    transition: all 0.3s ease;
    box-shadow: 0 10px 30px rgba(var(--primary-rgb), 0.3);
}

.submit-button:hover {
    transform: translateY(-3px);
    box-shadow: 0 15px 40px rgba(var(--primary-rgb), 0.5);
}

.submit-button:active {
    transform: translateY(-1px);
}

/* ========================================
   Footer
   ======================================== */

.footer {
    background: var(--secondary-color);
    border-top: 1px solid var(--border-color);
    padding: 60px 0 20px;
}

.footer-content {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 40px;
    margin-bottom: 40px;
}

.footer-section h4 {
    font-size: 18px;
    margin-bottom: 15px;
    color: var(--primary-color);
}

.footer-section p {
    font-size: 14px;
    color: var(--text-light);
    opacity: 0.8;
}

.footer-section ul {
    list-style: none;
}

.footer-section ul li {
    margin-bottom: 10px;
}

.footer-section ul li a {
    color: var(--text-light);
    text-decoration: none;
    font-size: 14px;
    transition: all 0.3s ease;
}

.footer-section ul li a:hover {
    color: var(--primary-color);
    margin-left: 5px;
}

.social-links {
    display: flex;
    gap: 15px;
}

.social-links a {
    width: 40px;
    height: 40px;
    border: 1px solid var(--border-color);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--primary-color);
    transition: all 0.3s ease;
    font-size: 18px;
}

.social-links a:hover {
    background: var(--primary-color);
    color: white;
    border-color: var(--primary-color);
    transform: scale(1.1);
}

.footer-bottom {
    text-align: center;
    padding-top: 20px;
    border-top: 1px solid var(--border-color);
    color: var(--text-light);
    font-size: 13px;
    opacity: 0.8;
}

/* ========================================
   Responsive Design
   ======================================== */

@media (max-width: 768px) {
    .nav-menu {
        gap: 15px;
        font-size: 14px;
    }

    .nav-link {
        font-size: 14px;
    }

    .hamburger {
        display: flex;
    }

    .services-grid {
        grid-template-columns: 1fr;
    }

    .portfolio-grid {
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    }

    .about-content {
        grid-template-columns: 1fr;
        gap: 40px;
    }

    .about-stats {
        grid-template-columns: 1fr;
    }

    .contact-content {
        grid-template-columns: 1fr;
        gap: 40px;
    }

    .hero-title {
        font-size: 36px;
    }

    .hero-subtitle {
        font-size: 18px;
    }

    .section-title {
        font-size: 32px;
        margin-bottom: 40px;
    }

    .footer-content {
        grid-template-columns: 1fr;
        gap: 30px;
    }
}

@media (max-width: 480px) {
    .nav-container {
        height: 60px;
    }

    .logo {
        font-size: 18px;
    }

    .hero {
        margin-top: 60px;
        height: auto;
        padding: 60px 0;
        min-height: 70vh;
    }

    .hero-title {
        font-size: 28px;
    }

    .hero-subtitle {
        font-size: 14px;
    }

    .services {
        padding: 60px 0;
    }

    .services-grid {
        gap: 20px;
    }

    .service-card {
        padding: 25px;
    }

    .section-title {
        font-size: 24px;
        margin-bottom: 30px;
    }

    .about-stats {
        grid-template-columns: 1fr;
    }

    .stat {
        padding: 15px;
    }

    .stat h4 {
        font-size: 28px;
    }

    .portfolio-grid {
        grid-template-columns: 1fr;
    }

    .contact-form {
        gap: 15px;
    }

    .form-group input,
    .form-group select,
    .form-group textarea {
        padding: 12px;
    }

    .cta-button {
        padding: 12px 40px;
        font-size: 14px;
    }
}

/* ========================================
   Hero Video Background
   ======================================== */

.hero-video {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    z-index: 1;
}

.hero-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(1, 12, 20, 0.65);
    z-index: 2;
}

.hero-content {
    position: relative;
    z-index: 10;
}

.hero-socials {
    display: flex;
    justify-content: center;
    gap: 30px;
    margin: 25px 0;
    flex-wrap: wrap;
}

.hero-socials a {
    width: 50px;
    height: 50px;
    border: 2px solid var(--primary-color);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--primary-color);
    transition: all 0.3s ease;
    font-size: 24px;
    background: rgba(var(--primary-rgb), 0.1);
}

.hero-socials a:hover {
    background: var(--primary-color);
    color: white;
    transform: translateY(-5px);
    box-shadow: 0 10px 25px rgba(var(--primary-rgb), 0.4);
}

.socials-button {
    display: inline-block;
    margin-top: 20px;
    padding: 12px 40px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    color: white;
    text-decoration: none;
    border-radius: 50px;
    font-weight: 600;
    font-size: 14px;
    transition: all 0.3s ease;
    border: 2px solid transparent;
    cursor: pointer;
    box-shadow: 0 10px 30px rgba(var(--primary-rgb), 0.3);
}

.socials-button:hover {
    transform: translateY(-3px);
    box-shadow: 0 15px 40px rgba(var(--primary-rgb), 0.5);
}

/* ========================================
   Portfolio Redirect Section
   ======================================== */

.portfolio {
    padding: 100px 0;
    background-color: var(--bg-dark);
    text-align: center;
}

.portfolio-redirect-text {
    font-size: 18px;
    color: var(--text-light);
    margin-bottom: 40px;
    opacity: 0.9;
}

.portfolio-redirect-btn {
    display: inline-block;
}

/* ========================================
   Artists Page Styles
   ======================================== */

.artists-hero {
    padding: 120px 20px 80px;
    background: linear-gradient(135deg, var(--bg-darker) 0%, var(--secondary-color) 100%);
    text-align: center;
    margin-top: 70px;
    position: relative;
    overflow: hidden;
}

.artists-hero::before {
    content: '';
    position: absolute;
    width: 300px;
    height: 300px;
    top: -50px;
    right: 10%;
    background: radial-gradient(circle, rgba(var(--accent-rgb), 0.2), transparent);
    filter: blur(40px);
}

.artists-title {
    font-size: 56px;
    font-family: 'Bebas Neue', sans-serif;
    color: var(--foam-color);
    margin-bottom: 15px;
    position: relative;
    z-index: 2;
}

.artists-subtitle {
    font-size: 18px;
    color: var(--text-light);
    opacity: 0.8;
    position: relative;
    z-index: 2;
}

.artists-container {
    padding: 80px 0;
    background-color: var(--bg-dark);
}

.artists-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
    gap: 40px;
    margin-top: 40px;
}

.artist-card {
    background: linear-gradient(135deg, rgba(var(--primary-rgb), 0.1), rgba(var(--accent-rgb), 0.06));
    border: 1px solid var(--border-color);
    border-radius: 15px;
    padding: 40px 30px;
    transition: all 0.3s ease;
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.artist-card:hover {
    transform: translateY(-15px);
    border-color: var(--primary-color);
    box-shadow: 0 25px 50px rgba(var(--primary-rgb), 0.25);
}

.artist-image {
    width: 120px;
    height: 120px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 60px;
    color: white;
    margin-bottom: 25px;
    box-shadow: 0 10px 30px rgba(var(--primary-rgb), 0.3);
}

.artist-name {
    font-size: 22px;
    color: var(--text-light);
    margin-bottom: 8px;
    font-family: 'Bebas Neue', sans-serif;
    letter-spacing: 1px;
}

.artist-role {
    font-size: 14px;
    color: var(--primary-color);
    font-weight: 600;
    margin-bottom: 12px;
}

.artist-bio {
    font-size: 14px;
    color: var(--text-light);
    opacity: 0.7;
    margin-bottom: 20px;
    line-height: 1.6;
}

.artist-discord {
    font-size: 13px;
    color: var(--accent-color);
    font-weight: 600;
    margin-bottom: 15px;
    font-family: 'Space Grotesk', sans-serif;
    letter-spacing: 0.5px;
}

.artist-links {
    display: flex;
    gap: 12px;
    justify-content: center;
    margin: 25px 0 0 0;
    flex-wrap: wrap;
    padding-top: 20px;
    border-top: 1px solid rgba(var(--primary-rgb), 0.2);
    width: 100%;
}

.artist-link {
    width: 45px;
    height: 45px;
    border: 2px solid var(--primary-color);
    border-radius: 8px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--primary-color);
    transition: all 0.3s ease;
    font-size: 20px;
    background: rgba(var(--primary-rgb), 0.05);
}

.artist-link:hover {
    background: var(--primary-color);
    color: white;
    transform: translateY(-3px);
    box-shadow: 0 8px 20px rgba(var(--primary-rgb), 0.3);
}

.portfolio-link {
    display: inline-block;
    margin-top: 15px;
    padding: 10px 30px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    color: white;
    text-decoration: none;
    border-radius: 25px;
    font-weight: 600;
    font-size: 13px;
    transition: all 0.3s ease;
}

.portfolio-link:hover {
    transform: translateY(-2px);
    box-shadow: 0 8px 20px rgba(var(--primary-rgb), 0.4);
}

/* ========================================
   Artist Portfolio Modal
   ======================================== */

.artist-portfolio-section {
    display: flex;
    align-items: center;
    justify-content: center;
    min-height: 100vh;
    padding: 40px 20px;
    background: var(--bg-dark);
    position: relative;
    margin-top: 70px;
}

.artist-portfolio-section h2 {
    font-size: 42px;
    font-family: 'Bebas Neue', sans-serif;
    color: var(--foam-color);
    margin-bottom: 50px;
    text-align: center;
    width: 100%;
}

.close-portfolio-btn {
    position: absolute;
    top: 30px;
    right: 30px;
    width: 50px;
    height: 50px;
    border: 2px solid var(--primary-color);
    background: rgba(var(--primary-rgb), 0.1);
    color: var(--primary-color);
    border-radius: 50%;
    font-size: 36px;
    cursor: pointer;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
}

.close-portfolio-btn:hover {
    background: var(--primary-color);
    color: white;
}

.portfolio-gallery {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 30px;
}

/* ========================================
   Socials Page Styles
   ======================================== */

.socials-hero {
    padding: 120px 20px 80px;
    background: linear-gradient(135deg, var(--bg-darker) 0%, var(--secondary-color) 100%);
    text-align: center;
    margin-top: 70px;
    position: relative;
    overflow: hidden;
}

.socials-hero::before {
    content: '';
    position: absolute;
    width: 300px;
    height: 300px;
    top: -50px;
    left: 15%;
    background: radial-gradient(circle, rgba(var(--primary-rgb), 0.2), transparent);
    filter: blur(40px);
}

.socials-title {
    font-size: 56px;
    font-family: 'Bebas Neue', sans-serif;
    color: var(--foam-color);
    margin-bottom: 15px;
    position: relative;
    z-index: 2;
}

.socials-subtitle {
    font-size: 18px;
    color: var(--text-light);
    opacity: 0.8;
    position: relative;
    z-index: 2;
}

.socials-container {
    padding: 80px 0;
    background-color: var(--bg-dark);
}

.socials-section {
    margin-bottom: 80px;
}

.socials-section-title {
    font-size: 36px;
    font-family: 'Bebas Neue', sans-serif;
    color: var(--foam-color);
    margin-bottom: 50px;
    text-align: center;
    letter-spacing: 2px;
}

/* Official Server Card */
.official-server-card {
    background: linear-gradient(135deg, rgba(var(--primary-rgb), 0.15), rgba(var(--accent-rgb), 0.1));
    border: 2px solid var(--primary-color);
    border-radius: 15px;
    padding: 60px 40px;
    text-align: center;
    max-width: 500px;
    margin: 0 auto;
    transition: all 0.3s ease;
}

.official-server-card:hover {
    transform: translateY(-10px);
    box-shadow: 0 25px 50px rgba(var(--primary-rgb), 0.25);
}

.server-icon {
    font-size: 80px;
    color: var(--primary-color);
    margin-bottom: 20px;
}

.official-server-card h3 {
    font-size: 28px;
    color: var(--foam-color);
    margin-bottom: 15px;
    font-family: 'Bebas Neue', sans-serif;
}

.official-server-card p {
    font-size: 15px;
    color: var(--text-light);
    margin-bottom: 30px;
    line-height: 1.6;
}

.social-button {
    display: inline-block;
    padding: 15px 50px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    color: white;
    text-decoration: none;
    border-radius: 50px;
    font-weight: 600;
    font-size: 16px;
    transition: all 0.3s ease;
    border: 2px solid transparent;
    cursor: pointer;
    box-shadow: 0 10px 30px rgba(var(--primary-rgb), 0.3);
}

.social-button:hover {
    transform: translateY(-3px);
    box-shadow: 0 15px 40px rgba(var(--primary-rgb), 0.5);
}

/* Artists Socials Grid */
.artists-socials-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 30px;
}

.artist-social-card {
    background: rgba(var(--primary-rgb), 0.08);
    border: 1px solid var(--border-color);
    border-radius: 12px;
    padding: 30px;
    text-align: center;
    transition: all 0.3s ease;
}

.artist-social-card:hover {
    border-color: var(--primary-color);
    background: rgba(var(--primary-rgb), 0.12);
    transform: translateY(-8px);
}

.artist-social-card h3 {
    font-size: 20px;
    color: var(--foam-color);
    margin-bottom: 8px;
    font-family: 'Bebas Neue', sans-serif;
}

.artist-role-small {
    font-size: 13px;
    color: var(--primary-color);
    font-weight: 600;
    margin-bottom: 20px;
}

.artist-social-links {
    display: flex;
    flex-direction: column;
    gap: 12px;
}

.social-icon-link {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 10px;
    padding: 12px;
    background: rgba(var(--primary-rgb), 0.1);
    border: 1px solid var(--border-color);
    border-radius: 8px;
    color: var(--primary-color);
    text-decoration: none;
    font-size: 14px;
    font-weight: 500;
    transition: all 0.3s ease;
}

.social-icon-link:hover {
    background: var(--primary-color);
    color: white;
    border-color: var(--primary-color);
    transform: translateX(5px);
}

.social-icon-link i {
    font-size: 16px;
}

/* Studio Socials Grid */
.studio-socials-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
    gap: 25px;
    max-width: 700px;
    margin: 0 auto;
}

.studio-social-link {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 15px;
    padding: 40px;
    background: linear-gradient(135deg, rgba(var(--primary-rgb), 0.12), rgba(var(--accent-rgb), 0.08));
    border: 1px solid var(--border-color);
    border-radius: 12px;
    text-decoration: none;
    color: var(--text-light);
    transition: all 0.3s ease;
}

.studio-social-link:hover {
    border-color: var(--primary-color);
    background: rgba(var(--primary-rgb), 0.15);
    transform: translateY(-8px);
    box-shadow: 0 15px 35px rgba(var(--primary-rgb), 0.2);
}

.studio-social-icon {
    width: 70px;
    height: 70px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 32px;
    color: white;
}

.studio-social-link span {
    font-weight: 600;
    font-size: 15px;
}

/* ========================================
   Responsive - Artists & Socials
   ======================================== */

@media (max-width: 768px) {
    .artists-title,
    .socials-title {
        font-size: 36px;
    }

    .artists-subtitle,
    .socials-subtitle {
        font-size: 16px;
    }

    .artist-card {
        padding: 30px 20px;
    }

    .artist-image {
        width: 100px;
        height: 100px;
        font-size: 50px;
    }

    .hero-socials {
        gap: 20px;
    }

    .hero-socials a {
        width: 45px;
        height: 45px;
        font-size: 20px;
    }

    .socials-section-title {
        font-size: 28px;
    }

    .official-server-card {
        padding: 40px 25px;
    }

    .official-server-card h3 {
        font-size: 24px;
    }

    .artists-socials-grid {
        grid-template-columns: 1fr;
    }

    .studio-socials-grid {
        grid-template-columns: repeat(2, 1fr);
    }
}

@media (max-width: 480px) {
    .artists-title,
    .socials-title {
        font-size: 28px;
    }

    .artists-container {
        padding: 50px 0;
    }

    .artists-grid {
        grid-template-columns: 1fr;
        gap: 25px;
    }

    .hero-socials {
        gap: 15px;
    }

    .hero-socials a {
        width: 40px;
        height: 40px;
        font-size: 18px;
    }

    .socials-button {
        padding: 10px 30px;
        font-size: 12px;
    }

    .studio-socials-grid {
        grid-template-columns: 1fr;
    }

    .social-icon-link {
        padding: 10px;
        font-size: 12px;
    }
}

/* ========================================
   Login & Editor Styles
   ======================================== */

.login-btn {
    position: fixed;
    top: 85px;
    right: 30px;
    width: 50px;
    height: 50px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    border: none;
    border-radius: 50%;
    cursor: pointer;
    color: white;
    font-size: 24px;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease;
    z-index: 999;
    box-shadow: 0 8px 25px rgba(var(--primary-rgb), 0.3);
}

.login-btn:hover {
    transform: scale(1.1);
    box-shadow: 0 12px 35px rgba(var(--primary-rgb), 0.5);
}


@media (max-width: 768px) {
    .editor-panel {
        width: 100%;
        height: auto;
        max-height: 70vh;
        bottom: 0;
        top: auto;
        border-left: none;
        border-top: 2px solid var(--border-color);
    }

    .login-btn {
        top: auto;
        bottom: 20px;
        right: 20px;
    }

    .login-container {
        width: 95%;
        padding: 40px 30px;
    }
}

@media (max-width: 480px) {
    .editor-panel {
        width: 100%;
    }

    .editor-tabs {
        flex-wrap: wrap;
    }

    .editor-tab-btn {
        flex: 1 1 50%;
    }

    .login-container {
        width: 95%;
        padding: 30px 20px;
    }

    .login-container h2 {
        font-size: 24px;
    }
}

/* ========================================
   Utility Classes
   ======================================== */

.hidden {
    display: none;
}

.visible {
    display: block;
}

/* ========================================
   Login Page Styles
   ======================================== */

.login-body,
.dashboard-body,
.admin-body,
.editor-body {
    background: linear-gradient(135deg, var(--bg-darker) 0%, var(--secondary-color) 100%);
}

.login-page,
.dashboard-page {
    position: relative;
    width: 100%;
    min-height: 100vh;
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
}

.login-background,
.dashboard-background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 0;
}

.login-bg-video,
.dashboard-bg-video {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.login-overlay,
.dashboard-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(1, 12, 20, 0.8);
    z-index: 1;
}

.login-container,
.dashboard-container {
    position: relative;
    z-index: 2;
    width: 100%;
    max-width: 500px;
    padding: 40px;
}

.dashboard-container {
    max-width: 600px;
}

.login-card,
.dashboard-header {
    background: rgba(3, 23, 35, 0.95);
    border: 1px solid var(--border-color);
    border-radius: 16px;
    padding: 50px 40px;
    backdrop-filter: blur(10px);
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.5);
}

.login-header,
.dashboard-header {
    text-align: center;
    margin-bottom: 40px;
}

.login-logo,
.dashboard-logo {
    width: 60px;
    height: 60px;
    margin-bottom: 20px;
    display: block;
}

.login-header h1,
.dashboard-header h1 {
    font-size: 32px;
    color: var(--primary-color);
    margin-bottom: 10px;
    letter-spacing: 2px;
}

.login-header p,
.dashboard-header p {
    color: var(--accent-color);
    font-size: 14px;
}

.form-group {
    margin-bottom: 20px;
}

.form-group label {
    display: block;
    margin-bottom: 8px;
    color: var(--text-light);
    font-size: 14px;
    font-weight: 500;
}

.form-group input,
.form-group select {
    width: 100%;
    padding: 12px 15px;
    background: rgba(12, 58, 82, 0.3);
    border: 1px solid var(--border-color);
    border-radius: 8px;
    color: var(--text-light);
    font-family: inherit;
    transition: all 0.3s ease;
}

.form-group input:focus,
.form-group select:focus {
    outline: none;
    box-shadow: 0 0 0 3px rgba(43, 179, 255, 0.2);
    border-color: var(--primary-color);
    background: rgba(12, 58, 82, 0.5);
}

.form-group input::placeholder {
    color: rgba(230, 246, 255, 0.5);
}

.btn-primary,
.btn-secondary {
    padding: 12px 24px;
    border: none;
    border-radius: 8px;
    font-family: inherit;
    font-size: 14px;
    font-weight: 600;
    cursor: pointer;
    transition: all 0.3s ease;
    display: inline-flex;
    align-items: center;
    gap: 8px;
}

.btn-primary {
    background: linear-gradient(135deg, var(--primary-color) 0%, var(--accent-color) 100%);
    color: var(--text-dark);
    width: 100%;
}

.btn-primary:hover {
    transform: translateY(-2px);
    box-shadow: 0 10px 30px rgba(43, 179, 255, 0.3);
}

.btn-secondary {
    background: rgba(43, 179, 255, 0.2);
    color: var(--primary-color);
    border: 1px solid var(--primary-color);
}

.btn-secondary:hover {
    background: rgba(43, 179, 255, 0.3);
    transform: translateY(-2px);
}

.btn-login {
    margin-bottom: 20px;
}

.forgot-password {
    text-align: center;
    margin: 15px 0;
}

.forgot-password a {
    color: var(--accent-color);
    text-decoration: none;
    font-size: 13px;
    transition: all 0.3s ease;
}

.forgot-password a:hover {
    color: var(--primary-color);
}

.divider {
    position: relative;
    text-align: center;
    margin: 30px 0;
    color: rgba(230, 246, 255, 0.5);
    font-size: 13px;
}

.divider::before,
.divider::after {
    content: '';
    position: absolute;
    top: 50%;
    width: 45%;
    height: 1px;
    background: var(--border-color);
}

.divider::before {
    left: 0;
}

.divider::after {
    right: 0;
}

.oauth-buttons {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 15px;
    margin: 20px 0;
}

.btn-oauth {
    padding: 12px;
    border: 1px solid var(--border-color);
    background: rgba(12, 58, 82, 0.4);
    color: var(--text-light);
    border-radius: 8px;
    cursor: pointer;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    font-size: 13px;
    font-weight: 600;
}

.btn-oauth:hover {
    background: rgba(43, 179, 255, 0.2);
    border-color: var(--primary-color);
}

.signup-prompt,
.login-prompt,
.form-description {
    text-align: center;
    color: var(--text-light);
    font-size: 13px;
    margin-top: 15px;
}

.signup-prompt a,
.login-prompt a {
    color: var(--primary-color);
    font-weight: 600;
    cursor: pointer;
    text-decoration: none;
}

.signup-prompt a:hover,
.login-prompt a:hover {
    color: var(--accent-color);
}

.login-form,
.signup-form,
.forgot-password-form {
    display: none;
}

.active-form {
    display: block !important;
}

.login-footer {
    text-align: center;
    color: rgba(230, 246, 255, 0.6);
    font-size: 12px;
    margin-top: 30px;
}

/* ========================================
   Dashboard Styles
   ======================================== */

.dashboard-options {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 30px;
    margin: 50px 0;
}

.option-card {
    background: rgba(3, 23, 35, 0.95);
    border: 1px solid var(--border-color);
    border-radius: 16px;
    padding: 40px 30px;
    text-align: center;
    backdrop-filter: blur(10px);
    transition: all 0.3s ease;
    cursor: pointer;
}

.option-card:hover {
    transform: translateY(-5px);
    box-shadow: 0 20px 60px rgba(43, 179, 255, 0.2);
    border-color: var(--primary-color);
}

.option-icon {
    font-size: 48px;
    color: var(--primary-color);
    margin-bottom: 20px;
}

.option-icon.admin {
    color: var(--accent-color);
}

.option-card h2 {
    font-size: 24px;
    margin-bottom: 10px;
    color: var(--text-light);
}

.option-card p {
    color: rgba(230, 246, 255, 0.7);
    font-size: 14px;
    margin-bottom: 25px;
    line-height: 1.6;
}

.option-card .btn-primary {
    margin-top: 20px;
}

.user-options {
    display: flex;
    gap: 20px;
    justify-content: center;
    margin-top: 40px;
}

.user-link {
    color: var(--primary-color);
    text-decoration: none;
    font-size: 14px;
    display: flex;
    align-items: center;
    gap: 8px;
    padding: 8px 16px;
    border: 1px solid var(--border-color);
    border-radius: 8px;
    transition: all 0.3s ease;
}

.user-link:hover {
    background: rgba(43, 179, 255, 0.1);
    border-color: var(--primary-color);
}

.user-link.logout {
    color: #ff6b6b;
    border-color: #ff6b6b;
}

.user-link.logout:hover {
    background: rgba(255, 107, 107, 0.1);
}

/* ========================================
   Admin Panel Styles
   ======================================== */

.admin-container {
    display: grid;
    grid-template-columns: 250px 1fr;
    min-height: 100vh;
    background: var(--bg-dark);
}

.admin-sidebar {
    background: rgba(3, 23, 35, 0.95);
    border-right: 1px solid var(--border-color);
    padding: 30px 20px;
    position: relative;
    overflow-y: auto;
    max-height: 100vh;
    backdrop-filter: blur(10px);
}

.sidebar-header {
    display: flex;
    align-items: center;
    gap: 15px;
    margin-bottom: 40px;
    padding-bottom: 20px;
    border-bottom: 1px solid var(--border-color);
}

.sidebar-logo {
    width: 40px;
    height: 40px;
}

.sidebar-header h2 {
    font-size: 18px;
    color: var(--primary-color);
    letter-spacing: 1px;
}

.sidebar-nav {
    display: flex;
    flex-direction: column;
    gap: 10px;
    margin-bottom: 40px;
}

.nav-item {
    display: flex;
    align-items: center;
    gap: 12px;
    padding: 12px 16px;
    color: rgba(230, 246, 255, 0.7);
    text-decoration: none;
    border-radius: 8px;
    transition: all 0.3s ease;
    font-size: 14px;
    cursor: pointer;
}

.nav-item:hover {
    background: rgba(43, 179, 255, 0.1);
    color: var(--primary-color);
}

.nav-item.active {
    background: linear-gradient(90deg, rgba(43, 179, 255, 0.2), transparent);
    color: var(--primary-color);
    border-left: 3px solid var(--primary-color);
    padding-left: 13px;
}

.nav-item i {
    width: 20px;
    text-align: center;
}

.sidebar-footer {
    display: flex;
    flex-direction: column;
    gap: 10px;
    padding-top: 20px;
    border-top: 1px solid var(--border-color);
}

.admin-main {
    padding: 40px;
    overflow-y: auto;
    max-height: 100vh;
}

.admin-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 40px;
    padding-bottom: 20px;
    border-bottom: 1px solid var(--border-color);
}

.admin-header h1 {
    font-size: 28px;
    color: var(--text-light);
}

.admin-user-info {
    display: flex;
    align-items: center;
    gap: 12px;
}

.admin-avatar {
    width: 40px;
    height: 40px;
    border-radius: 50%;
}

.admin-section {
    display: none;
}

.admin-section.active {
    display: block;
    animation: fadeIn 0.3s ease;
}

@keyframes fadeIn {
    from { opacity: 0; }
    to { opacity: 1; }
}

.section-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 30px;
}

.section-header h2 {
    font-size: 22px;
    color: var(--text-light);
}

.stats-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 20px;
    margin-bottom: 40px;
}

.stat-card {
    background: rgba(12, 58, 82, 0.4);
    border: 1px solid var(--border-color);
    border-radius: 12px;
    padding: 25px;
    display: flex;
    gap: 20px;
}

.stat-icon {
    font-size: 32px;
    color: var(--primary-color);
    width: 60px;
    height: 60px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: rgba(43, 179, 255, 0.1);
    border-radius: 8px;
    flex-shrink: 0;
}

.stat-content {
    flex: 1;
}

.stat-content h3 {
    font-size: 13px;
    color: rgba(230, 246, 255, 0.7);
    margin-bottom: 8px;
    text-transform: uppercase;
    letter-spacing: 1px;
}

.stat-value {
    font-size: 28px;
    color: var(--primary-color);
    margin-bottom: 5px;
}

.stat-change {
    font-size: 12px;
    color: var(--accent-color);
}

.dashboard-sections {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 30px;
}

.info-section {
    background: rgba(12, 58, 82, 0.4);
    border: 1px solid var(--border-color);
    border-radius: 12px;
    padding: 25px;
}

.info-section h3 {
    font-size: 16px;
    margin-bottom: 20px;
    color: var(--text-light);
}

.recent-activity {
    display: flex;
    flex-direction: column;
    gap: 15px;
}

.activity-item {
    display: flex;
    gap: 15px;
    padding: 15px;
    background: rgba(3, 23, 35, 0.5);
    border-radius: 8px;
}

.activity-item i {
    color: var(--primary-color);
    font-size: 18px;
    width: 25px;
    flex-shrink: 0;
}

.activity-content p {
    font-size: 13px;
    color: var(--text-light);
    margin: 0;
}

.activity-content .time {
    font-size: 11px;
    color: rgba(230, 246, 255, 0.5);
    margin-top: 5px;
    display: block;
}

.users-table,
.audit-table {
    background: rgba(12, 58, 82, 0.3);
    border: 1px solid var(--border-color);
    border-radius: 12px;
    overflow: hidden;
}

.table-header {
    display: grid;
    grid-template-columns: 1.5fr 1.5fr 1.5fr 2fr 1fr 1fr;
    background: rgba(3, 23, 35, 0.8);
    padding: 20px;
    font-weight: 600;
    color: var(--primary-color);
    font-size: 13px;
    border-bottom: 1px solid var(--border-color);
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

.audit-table .table-header {
    grid-template-columns: 1.5fr 1.5fr 1.5fr 2fr;
}

.table-row {
    display: grid;
    grid-template-columns: 1.5fr 1.5fr 1.5fr 2fr 1fr 1fr;
    padding: 15px 20px;
    border-bottom: 1px solid var(--border-color);
    align-items: center;
    transition: background 0.3s ease;
}

.audit-table .table-row {
    grid-template-columns: 1.5fr 1.5fr 1.5fr 2fr;
}

.table-row:hover {
    background: rgba(43, 179, 255, 0.1);
}

.table-cell {
    font-size: 13px;
    color: var(--text-light);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.table-cell.password {
    letter-spacing: 2px;
    color: var(--accent-color);
    font-weight: 500;
}

.role-cell select {
    background: rgba(12, 58, 82, 0.6);
    border: 1px solid var(--border-color);
    color: var(--text-light);
    padding: 6px 10px;
    border-radius: 4px;
    cursor: pointer;
    font-size: 12px;
}

.table-cell.actions {
    display: flex;
    gap: 10px;
}

.btn-small {
    padding: 6px 12px;
    font-size: 12px;
    border: 1px solid var(--border-color);
    background: rgba(43, 179, 255, 0.1);
    color: var(--primary-color);
    border-radius: 4px;
    cursor: pointer;
    transition: all 0.3s ease;
}

.btn-small:hover {
    background: rgba(43, 179, 255, 0.2);
}

.btn-delete {
    border-color: #ff6b6b;
    color: #ff6b6b;
    background: rgba(255, 107, 107, 0.1);
}

.btn-delete:hover {
    background: rgba(255, 107, 107, 0.2);
}

.analytics-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 20px;
}

.analytics-card {
    background: rgba(12, 58, 82, 0.4);
    border: 1px solid var(--border-color);
    border-radius: 12px;
    padding: 25px;
}

.analytics-card h3 {
    font-size: 16px;
    margin-bottom: 20px;
    color: var(--text-light);
}

.analytics-stat {
    display: flex;
    flex-direction: column;
    gap: 15px;
}

.stat-item {
    display: flex;
    justify-content: space-between;
    padding-bottom: 10px;
    border-bottom: 1px solid rgba(43, 179, 255, 0.1);
}

.stat-item span {
    color: rgba(230, 246, 255, 0.7);
    font-size: 13px;
}

.stat-item strong {
    color: var(--primary-color);
    font-size: 14px;
}

.visits-chart-container {
    background: rgba(12, 58, 82, 0.4);
    border: 1px solid var(--border-color);
    border-radius: 12px;
    padding: 30px;
    margin-bottom: 30px;
}

#visitsChart {
    max-height: 400px;
}

.chart-info {
    text-align: center;
    color: rgba(230, 246, 255, 0.6);
    font-size: 12px;
    margin-top: 20px;
}

.chart-hover-info {
    background: rgba(3, 23, 35, 0.8);
    border: 1px solid var(--primary-color);
    border-radius: 8px;
    padding: 15px;
    margin-top: 15px;
    color: var(--text-light);
}

.chart-hover-info p {
    margin: 5px 0;
    font-size: 13px;
}

.settings-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 30px;
}

.settings-card {
    background: rgba(12, 58, 82, 0.4);
    border: 1px solid var(--border-color);
    border-radius: 12px;
    padding: 30px;
}

.setting-item {
    margin-bottom: 25px;
    display: flex;
    align-items: center;
    gap: 12px;
}

.setting-item input[type="text"],
.setting-item input[type="email"],
.setting-item input[type="url"] {
    flex: 1;
    padding: 10px 15px;
    background: rgba(3, 23, 35, 0.8);
    border: 1px solid var(--border-color);
    border-radius: 6px;
    color: var(--text-light);
    font-size: 13px;
}

.setting-item input[type="checkbox"] {
    width: 20px;
    height: 20px;
    cursor: pointer;
    accent-color: var(--primary-color);
}

.setting-item label {
    display: flex;
    align-items: center;
    gap: 10px;
    cursor: pointer;
    margin: 0;
}

.setting-item > div:last-child {
    margin-top: auto;
}

.modal {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.7);
    z-index: 1000;
    backdrop-filter: blur(5px);
}

.modal-content {
    background: rgba(3, 23, 35, 0.95);
    border: 1px solid var(--border-color);
    border-radius: 16px;
    padding: 40px;
    max-width: 500px;
    width: 90%;
    backdrop-filter: blur(10px);
    animation: slideUp 0.3s ease;
}

@keyframes slideUp {
    from {
        opacity: 0;
        transform: translateY(50px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

.modal-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 30px;
    padding-bottom: 15px;
    border-bottom: 1px solid var(--border-color);
}

.modal-header h2 {
    color: var(--text-light);
    font-size: 20px;
}

.modal-close {
    background: none;
    border: none;
    color: var(--text-light);
    font-size: 28px;
    cursor: pointer;
    transition: color 0.3s;
}

.modal-close:hover {
    color: var(--primary-color);
}

#addUserForm {
    display: flex;
    flex-direction: column;
    gap: 20px;
}

#addUserForm button {
    margin-top: 10px;
}

/* ========================================
   Website Editor Styles
   ======================================== */

.editor-container {
    display: flex;
    flex-direction: column;
    height: 100vh;
    background: var(--bg-dark);
}

.editor-header {
    background: rgba(3, 23, 35, 0.95);
    border-bottom: 1px solid var(--border-color);
    padding: 20px 30px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    z-index: 100;
}

.editor-header-left {
    display: flex;
    align-items: center;
    gap: 15px;
}

.editor-logo {
    width: 32px;
    height: 32px;
}

.editor-header-left h1 {
    font-size: 20px;
    color: var(--primary-color);
}

.editor-header-right {
    display: flex;
    gap: 12px;
}

.editor-workspace {
    display: grid;
    grid-template-columns: 250px 1fr 300px;
    flex: 1;
    overflow: hidden;
    gap: 0;
}

.editor-sidebar {
    background: rgba(3, 23, 35, 0.8);
    border-right: 1px solid var(--border-color);
    overflow-y: auto;
    padding: 20px;
}

.editor-sidebar.right-sidebar {
    border-right: none;
    border-left: 1px solid var(--border-color);
}

.sidebar-section {
    margin-bottom: 30px;
}

.sidebar-section h3 {
    font-size: 14px;
    color: var(--primary-color);
    margin-bottom: 15px;
    text-transform: uppercase;
    letter-spacing: 1px;
}

.elements-list {
    display: flex;
    flex-direction: column;
    gap: 8px;
}

.element-item {
    display: flex;
    align-items: center;
    gap: 10px;
    padding: 12px;
    background: rgba(12, 58, 82, 0.4);
    border: 1px solid var(--border-color);
    border-radius: 8px;
    color: var(--text-light);
    cursor: move;
    font-size: 13px;
    transition: all 0.3s ease;
}

.element-item:hover {
    background: rgba(43, 179, 255, 0.2);
    border-color: var(--primary-color);
}

.element-item i {
    color: var(--primary-color);
    width: 20px;
    text-align: center;
}

.editor-canvas {
    background: linear-gradient(135deg, var(--bg-darker) 0%, var(--bg-dark) 100%);
    overflow-y: auto;
    padding: 40px;
    position: relative;
}

.canvas-content {
    background: rgba(12, 58, 82, 0.2);
    border: 2px dashed var(--border-color);
    border-radius: 12px;
    padding: 40px;
    min-height: 100%;
    transition: all 0.3s ease;
}

.canvas-placeholder {
    text-align: center;
    color: rgba(230, 246, 255, 0.5);
    padding: 80px 20px;
}

.canvas-placeholder i {
    font-size: 64px;
    margin-bottom: 20px;
    display: block;
}

.canvas-placeholder p {
    font-size: 16px;
}

.editor-element {
    position: relative;
    background: rgba(43, 179, 255, 0.05);
    border: 1px solid var(--accent-color);
    border-radius: 8px;
    padding: 20px;
    margin: 15px 0;
    cursor: move;
    transition: all 0.3s ease;
}

.editor-element:hover {
    background: rgba(43, 179, 255, 0.1);
    box-shadow: 0 0 0 2px rgba(43, 179, 255, 0.3);
}

.editor-element.selected {
    background: rgba(43, 179, 255, 0.15);
    box-shadow: 0 0 0 3px rgba(43, 179, 255, 0.4);
    border-color: var(--primary-color);
}

.element-content {
    color: var(--text-light);
}

.element-remove-btn {
    position: absolute;
    top: 5px;
    right: 5px;
    background: #ff6b6b;
    color: white;
    border: none;
    width: 28px;
    height: 28px;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    opacity: 0;
    transition: all 0.3s ease;
}

.editor-element:hover .element-remove-btn {
    opacity: 1;
}

.element-remove-btn:hover {
    background: #ff5252;
    transform: scale(1.1);
}

.properties-panel {
    display: flex;
    flex-direction: column;
    gap: 15px;
}

.property-item {
    display: flex;
    flex-direction: column;
    gap: 8px;
}

.property-item label {
    color: var(--primary-color);
    font-size: 12px;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

.property-item p {
    color: var(--text-light);
    font-size: 13px;
    margin: 0;
}

.style-controls {
    display: flex;
    flex-direction: column;
    gap: 15px;
}

.control-group {
    display: flex;
    flex-direction: column;
    gap: 8px;
}

.control-group label {
    color: var(--primary-color);
    font-size: 12px;
    text-transform: uppercase;
}

.control-group input[type="color"],
.control-group input[type="range"],
.control-group input[type="number"] {
    background: rgba(12, 58, 82, 0.6);
    border: 1px solid var(--border-color);
    color: var(--text-light);
    padding: 8px;
    border-radius: 4px;
    font-family: inherit;
}

.control-group span {
    color: rgba(230, 246, 255, 0.6);
    font-size: 12px;
}

.save-status {
    position: fixed;
    bottom: 20px;
    right: 20px;
    background: rgba(43, 179, 255, 0.9);
    color: white;
    padding: 12px 20px;
    border-radius: 8px;
    display: flex;
    align-items: center;
    gap: 8px;
    font-size: 13px;
    opacity: 0;
    transition: opacity 0.3s ease;
    z-index: 500;
}

.save-status.show {
    opacity: 1;
}

.preview-modal {
    max-width: 90%;
    max-height: 90vh;
    width: auto;
}

.preview-content {
    max-height: 70vh;
    overflow-y: auto;
}

/* ========================================
   Notification Styles
   ======================================== */

.notification {
    position: fixed;
    top: 20px;
    right: 20px;
    padding: 15px 25px;
    border-radius: 8px;
    font-size: 14px;
    font-weight: 600;
    z-index: 10000;
    opacity: 0;
    transform: translateX(400px);
    transition: all 0.3s ease;
}

.notification.show {
    opacity: 1;
    transform: translateX(0);
}

.notification-success {
    background: rgba(76, 175, 80, 0.9);
    color: white;
}

.notification-error {
    background: rgba(193, 67, 79, 0.9);
    color: white;
}

.notification-info {
    background: rgba(43, 179, 255, 0.9);
    color: white;
}

/* ========================================
   Login Button Update
   ======================================== */

.login-btn {
    background: linear-gradient(135deg, rgba(43, 179, 255, 0.4) 0%, rgba(0, 209, 255, 0.3) 100%);
    border: 1px solid var(--primary-color);
    color: var(--primary-color);
    padding: 8px 16px;
    border-radius: 6px;
    cursor: pointer;
    font-size: 13px;
    font-weight: 600;
    display: flex;
    align-items: center;
    gap: 6px;
    transition: all 0.3s ease;
    white-space: nowrap;
}

.login-btn:hover {
    background: linear-gradient(135deg, rgba(43, 179, 255, 0.6) 0%, rgba(0, 209, 255, 0.5) 100%);
    box-shadow: 0 0 20px rgba(43, 179, 255, 0.4);
    transform: translateY(-2px);
}

/* ========================================
   Footer Styles
   ======================================== */

.footer {
    background: rgba(3, 23, 35, 0.95);
    border-top: 1px solid var(--border-color);
    padding: 60px 0 20px;
    margin-top: 80px;
    color: var(--text-light);
}

.footer-content {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));
    gap: 40px;
    margin-bottom: 40px;
}

.footer-section {
    display: flex;
    flex-direction: column;
    gap: 15px;
}

.footer-section h4 {
    font-size: 16px;
    color: var(--primary-color);
    text-transform: uppercase;
    letter-spacing: 1px;
    margin-bottom: 10px;
}

.footer-section p {
    font-size: 13px;
    line-height: 1.8;
    color: rgba(230, 246, 255, 0.7);
}

.footer-section ul {
    list-style: none;
    padding: 0;
    margin: 0;
}

.footer-section li {
    margin-bottom: 10px;
}

.footer-section a {
    color: rgba(230, 246, 255, 0.7);
    text-decoration: none;
    font-size: 13px;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    gap: 8px;
}

.footer-section a:hover {
    color: var(--primary-color);
    transform: translateX(5px);
}

.footer-section i {
    font-size: 16px;
}

.footer-bottom {
    border-top: 1px solid var(--border-color);
    padding-top: 20px;
    text-align: center;
    color: rgba(230, 246, 255, 0.5);
    font-size: 12px;
}

/* ========================================
   Legal Pages Styles
   ======================================== */

.legal-section {
    min-height: calc(100vh - 80px);
    padding: 80px 0 60px;
    background: linear-gradient(135deg, var(--bg-darker) 0%, var(--bg-dark) 100%);
}

.legal-container {
    max-width: 900px;
}

.legal-header {
    text-align: center;
    margin-bottom: 60px;
}

.legal-header h1 {
    font-size: 48px;
    color: var(--primary-color);
    margin-bottom: 10px;
    letter-spacing: 2px;
}

.legal-updated {
    color: var(--accent-color);
    font-size: 13px;
}

.legal-content {
    background: rgba(12, 58, 82, 0.2);
    border: 1px solid var(--border-color);
    border-radius: 12px;
    padding: 40px;
    margin-bottom: 40px;
}

.legal-content h2 {
    font-size: 22px;
    color: var(--primary-color);
    margin-top: 40px;
    margin-bottom: 20px;
    padding-bottom: 10px;
    border-bottom: 1px solid rgba(43, 179, 255, 0.2);
}

.legal-content h2:first-child {
    margin-top: 0;
}

.legal-content h3 {
    font-size: 16px;
    color: var(--accent-color);
    margin-top: 20px;
    margin-bottom: 15px;
}

.legal-content p {
    font-size: 14px;
    line-height: 1.8;
    color: rgba(230, 246, 255, 0.85);
    margin-bottom: 15px;
}

.legal-content ul {
    list-style: none;
    padding-left: 0;
    margin-bottom: 20px;
}

.legal-content li {
    font-size: 14px;
    line-height: 1.8;
    color: rgba(230, 246, 255, 0.85);
    margin-bottom: 10px;
    padding-left: 25px;
    position: relative;
}

.legal-content li::before {
    content: '';
    position: absolute;
    left: 0;
    color: var(--primary-color);
}

.legal-footer-note {
    background: rgba(43, 179, 255, 0.1);
    border-left: 3px solid var(--primary-color);
    padding: 20px;
    border-radius: 8px;
    margin-top: 40px;
}

.legal-footer-note p {
    margin: 0;
    font-size: 13px;
    color: var(--text-light);
}

.legal-footer-links {
    display: flex;
    gap: 20px;
    justify-content: center;
    flex-wrap: wrap;
}

.legal-footer-links .btn-secondary {
    padding: 12px 24px;
}

/* ========================================
   Responsive Footer & Legal
   ======================================== */

@media (max-width: 768px) {
    .footer-content {
        grid-template-columns: 1fr 1fr;
        gap: 30px;
    }

    .legal-section {
        padding: 40px 0 40px;
    }

    .legal-header h1 {
        font-size: 32px;
    }

    .legal-content {
        padding: 30px 20px;
    }

    .legal-content h2 {
        font-size: 18px;
    }

    .legal-content h3 {
        font-size: 14px;
    }

    .legal-content p,
    .legal-content li {
        font-size: 13px;
    }

    .legal-footer-links {
        flex-direction: column;
    }

    .legal-footer-links .btn-secondary {
        width: 100%;
        justify-content: center;
    }
}

@media (max-width: 480px) {
    .footer-content {
        grid-template-columns: 1fr;
        gap: 25px;
    }

    .legal-header h1 {
        font-size: 24px;
    }

    .legal-content {
        padding: 20px;
        border-radius: 8px;
    }

    .legal-content h2 {
        font-size: 16px;
        margin-top: 30px;
    }

    .legal-content h3 {
        font-size: 13px;
    }

    .legal-updated {
        font-size: 12px;
    }
}

/* ========================================
   Responsive Design
   ======================================== */

@media (max-width: 1024px) {
    .admin-container {
        grid-template-columns: 200px 1fr;
    }

    .editor-workspace {
        grid-template-columns: 200px 1fr 260px;
    }

    .table-header,
    .table-row {
        grid-template-columns: 1fr 1fr 1fr;
        gap: 10px;
    }

    .table-cell {
        word-break: break-word;
        white-space: normal;
        overflow: visible;
        text-overflow: clip;
    }
}

@media (max-width: 768px) {
    .admin-container {
        grid-template-columns: 1fr;
        min-height: auto;
    }

    .admin-sidebar {
        position: fixed;
        left: 0;
        top: 0;
        width: 250px;
        height: 100vh;
        z-index: 999;
        transform: translateX(-100%);
        transition: transform 0.3s ease;
        max-height: 100vh;
    }

    .admin-sidebar.active {
        transform: translateX(0);
    }

    .editor-workspace {
        grid-template-columns: 1fr;
    }

    .editor-sidebar {
        display: none;
    }

    .editor-sidebar.active {
        display: block;
        position: absolute;
        left: 0;
        top: 60px;
        width: 250px;
        z-index: 200;
    }

    .dashboard-options {
        grid-template-columns: 1fr;
    }

    .oauth-buttons {
        grid-template-columns: 1fr;
    }

    .login-card,
    .dashboard-container {
        padding: 30px 20px;
    }

    .stats-grid {
        grid-template-columns: 1fr;
    }

    .dashboard-sections {
        grid-template-columns: 1fr;
    }

    .settings-grid,
    .analytics-grid {
        grid-template-columns: 1fr;
    }
}

@media (max-width: 480px) {
    .login-card {
        padding: 20px;
    }

    .editor-header {
        flex-direction: column;
        gap: 15px;
        padding: 15px;
    }

    .editor-header-right {
        width: 100%;
        flex-wrap: wrap;
    }

    .btn-primary,
    .btn-secondary {
        padding: 10px 15px;
        font-size: 12px;
    }

    .stat-card {
        flex-direction: column;
    }

    .stat-icon {
        width: 50px;
        height: 50px;
    }
}
